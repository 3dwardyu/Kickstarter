<h1>Projects#new</h1>
<p>Find me in app/views/projects/new.html.erb</p>

<%= form_for @project do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :description %>
<%= f.text_area :description %>

<%= f.label :goal %>
<%= f.number_field :goal %>

<%= f.label :image %>
<%= f.text_field :image %>


<%= f.submit "Start Project" %>
<% end %>
