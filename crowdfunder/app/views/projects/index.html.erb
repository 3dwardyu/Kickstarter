<% if user_signed_in? %>
<li>
  <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
</li>
<% else %>
<li>
  <%= link_to('Login', new_user_session_path)  %>
</li>
<% end %>
<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>
<% @projects.each do |p| %>
<h2><%= p.title %></h2>
<p><%= p.description %></p>
<p><%= image_tag p.image %></p>
<% end %>
